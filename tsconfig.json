{
  "include": ["src/**/*.js", "src/**/*.ts", "src/**/*.tsx"],
  "exclude": ["build", "node_modules"],
  "typeAcquisition": {
    "exclude": ["crc-32", "fast-safe-stringify"]
  },
  "compilerOptions": {
    "baseUrl": "src",
    "jsx": "react-jsx",
    "target": "es6",
    "module": "commonjs",
    "esModuleInterop": true,
    "isolatedModules": false
    //  "strict": true,
  }
}
