ul.results
  li
    a
      display block
      padding 5px 4px
      text-decoration none
      &:hover
        background #f2f2f2
      .description
        color #5c5c5c
        font-size 13px
    &:not(:last-child) a
      border-bottom 1px solid #b6b6b6

  .snippet
    .highlight
      // text-decoration underline
      background: #ffdc89;
      border-radius: 2px;
      margin: 0px -1px;
      padding: 1px 3px;
