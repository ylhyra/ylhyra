html, body
  min-height 100vh
body
  sans-serif()
  background: rgb(255, 254, 252);
  font-size 14px

.rootpage-Ylhýra
  #title
    display none
    margin-bottom: 20px;


#title
  font-size 13px
  margin-bottom: 10px;
  .title-part
    border-radius: 12px;
    font-family:sans-serif;
    font-weight normal
    &:after
      content ' › '
      padding: 2px 1px;
      color: #b9b9b9;
    &.namespace
      color:gray;
    &.last
      color black
      font-weight bold
      &:after
        content ''
  .title-seperator
    font-size 0
    opacity 0

.subpages
  display none

.mw-redirectedfrom
  color: gray;
  font-size: 12px;
  body:not(.mw-editable) &
    display none

hr
  margin 40px 20px
  border none
  border-bottom 1px solid #e6e6e6

.center
  display flex
  justify-content space-around

.container
  position: relative

.supports-touch
  -webkit-touch-callout: none /* iOS Safari */
  -webkit-user-select: none /* Safari */
  -khtml-user-select: none /* Konqueror HTML */
  -moz-user-select: none /* Firefox */
  -ms-user-select: none /* Internet Explorer/Edge */
  user-select: none /* Non-prefixed version, currently */

.mw-parser-output
  line-height 1.7
  letter-spacing 0.1px
  margin: 0;
  sans-serif()
  letter-spacing: 0.1px;
  line-height: 1.5;
  font-size 16px
  h1,h2,h3,h4,h5,h6
    sans-serif()
  .ns-0.action-view &
    font-size: 15px;


.ylhyra-text
  position: relative


.text-reset
  color: black;
  font-size: 14px;
  line-height: 1.3em;
  letter-spacing: 0px;
  text-transform: initial;
  font-style: initial;
  font-weight: normal;
  margin: 0;
  padding: 0;
  text-align: left;

code
  background: #f1f1f1;
  border: 1px solid #dedede;
  border-radius: 3px;
  padding: 2px 4px;

.small-text-block
  line-height: 1.4;
  display: block;

.sans-serif
  sans-serif()

.div
  display block
