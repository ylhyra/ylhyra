fieldset
  border 1px solid #ddd
  margin 1em 0
#mw-js-message
  display none
#search-form
  margin-bottom 1em
  input
    border: 1px solid #ddd;
    box-sizing: border-box;
    font: inherit;
    font-size: medium;
    margin: 0;
    outline: none;
    padding: 5px 0;
    width: 107px;
    border-radius: 3px 0 0 3px;
    padding-left: 4px;
  button
    border: 1px solid #ddd;
    background: transparent;
    cursor: pointer;
    margin: 0;
    outline: none;
    padding: 3px;
    vertical-align: bottom;
    border-radius: 0 3px 3px 0;
    border-left: 0;
    height: 30px;
  img
    display block
    width 14px
.mw-editsection
  display none
  font-size small
  font-weight 300
.mw-editsection-bracket
  display none
.mw-collapsible-toggle
  margin-left 1em
.mw-collapsible-bracket
  display none
button
  &::-moz-focus-inner
    border 0
    padding 0
#footer-bottom
  border-top 1px solid #ddd
  font-size small
  padding 1em
.footer-icons
  float right
.footer-places
  padding 0
  li
    display inline
    margin-right 1em


/*
  VisualEditor
*/
.ve-ce-linkAnnotation.ve-ce-annotation-active.external
  padding-right none
