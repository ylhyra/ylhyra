{
  "name": "ylhyra",
  "version": "0.3.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git://github.com/ylhyra/ylhyra.git"
  },
  "license": "MIT",
  "private": false,
  "homepage": "https://ylhyra.is",
  "scripts": {
    "start2": "export BROWSER=none && react-scripts start",
    "start": "export BROWSER=none && export NODE_ENV=development && npx webpack serve --config scripts/webpack/app.webpack.js",
    "links": "./../ylhyra_content/scripts/cleanup.sh && export NODE_ENV=production && npm run build_server && npm run links_only",
    "links_only": "node build/server/ylhyra_server.js --generate-links",
    "chapters": "npm run links && node build/server/ylhyra_server.js --sort_course_chapters",
    "sitemap": "npm run links && npm run sitemap_only",
    "sitemap_only": "node build/server/ylhyra_server.js --sitemap",
    "vocabulary": "npm run build_server && npm run vocabulary_only",
    "vocabulary_es": "export DECK='_es' && npm run build_server && npm run vocabulary_only",
    "vocabulary_da": "export DECK='_da' && npm run build_server && npm run vocabulary_only",
    "vocabulary_no": "export DECK='_no' && npm run build_server && npm run vocabulary_only",
    "vocabulary_only": "node build/server/ylhyra_server.js --import-vocabulary",
    "vocabulary_testing": "node build/server/ylhyra_server.js --import-vocabulary --testing",
    "generate_sentences": "npm run build_server && node build/server/ylhyra_server.js --generate-sentences",
    "prerender": "npm run build_server && npm run prerender_only",
    "prerender_only": "export NODE_ENV=production && rm -f build/prerender/* && node build/server/ylhyra_server.js --prerender",
    "prerender_single": "npm run build_server && node build/server/ylhyra_server.js --prerender",
    "build_app": "./scripts/app.build.sh",
    "build_server": "export NODE_ENV=production && webpack --config scripts/webpack/server.webpack.js",
    "server2": "export NODE_ENV=development && webpack --config scripts/webpack/server.webpack.js --color --watch",
    "server": "export NODE_ENV=development &&  nodemon --exec 'webpack --config scripts/webpack/server.webpack.js --color && node build/server/development/ylhyra_server.development.js' --watch src/server --watch package.json",
    "build_all": "./scripts/build_all.sh",
    "push_to_server": "./scripts/push_to_server.sh",
    "deploy": "./scripts/deploy.sh",
    "audio1": "./scripts/audio_processing/Pronunciation_step_1.sh",
    "audio3": "./scripts/audio_processing/Pronunciation_step_3.sh",
    "test": "jest",
    "test2": "node -r esm test/vocabulary/vocabulary.test.js",
    "start_test_server": "export SERVER_PORT=9999 && node build/server/ylhyra_server.js"
  },
  "proxy": "http://localhost:9123",
  "dependencies": {
    "@gkt/microphone": "^1.0.3",
    "@google-cloud/translate": "^6.2.2",
    "@hcaptcha/react-hcaptcha": "^0.3.6",
    "@paypal/paypal-js": "^4.0.6",
    "argon2": "^0.28.2",
    "array-sugar": "^1.2.2",
    "atob": "^2.1.2",
    "audio-react-recorder": "^1.0.4",
    "axios": "^0.21.1",
    "axios-retry": "^3.1.9",
    "btoa": "^1.2.1",
    "chalk": "^4.1.1",
    "cologne-phonetic": "^1.0.1",
    "cookie-session": "^1.4.0",
    "core-js": "^3.14.0",
    "cors": "^2.8.5",
    "critical": "^4.0.0",
    "diacritics": "^1.3.0",
    "diff": "^5.0.0",
    "dotenv": "^10.0.0",
    "emoji-strip": "^1.0.1",
    "es6-crawler-detect": "^3.1.3",
    "esm": "^3.2.25",
    "express": "^4.17.1",
    "express-rate-limit": "^5.3.0",
    "express-slow-down": "^1.4.0",
    "express-useragent": "^1.0.15",
    "express-ws": "^4.0.0",
    "file-extension": "^4.0.5",
    "form-data": "^3.0.0",
    "formik": "^2.2.9",
    "globalthis": "^1.0.2",
    "html-entities": "^1.4.0",
    "import-js": "^4.0.2",
    "install": "^0.13.0",
    "is-boolean-attribute": "0.0.1",
    "is-empty-object": "^1.1.1",
    "jquery": "^3.6.0",
    "js-sha256": "^0.9.0",
    "js-yaml": "^4.1.0",
    "json-stable-stringify": "^1.0.1",
    "line-by-line": "^0.1.6",
    "lz-string": "^1.4.4",
    "marked": "^2.0.7",
    "mini-css-extract-plugin": "^1.6.0",
    "minimist": "^1.2.5",
    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "node-sass": "^6.0.0",
    "nodemailer": "^6.6.1",
    "nodemon-webpack-plugin": "^4.5.2",
    "npm": "^7.20.6",
    "omit-empty": "^1.0.0",
    "performance-now": "^2.1.0",
    "putout": "^19.0.0",
    "react": "^17.0.2",
    "react-attr-converter": "^0.3.1",
    "react-dom": "^17.0.2",
    "react-mic": "^12.4.6",
    "react-redux": "^7.2.4",
    "react-sound": "^1.2.0",
    "react-tap-or-click": "^0.2.1",
    "react-textarea-autosize": "^8.3.3",
    "redux": "^4.1.0",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.3.0",
    "regenerator-runtime": "^0.13.7",
    "request": "^2.88.2",
    "request-ip": "^2.1.3",
    "sass": "^1.37.0",
    "shortid": "^2.2.16",
    "source-map-support": "^0.5.19",
    "sqlstring": "^2.3.2",
    "string-hash": "^1.1.3",
    "string-similarity": "^4.0.4",
    "tableify": "^1.1.0",
    "twitter": "^1.7.1",
    "typeset": "^0.3.1",
    "underscore": "^1.13.1",
    "vhost": "^3.0.2",
    "webpack-node-externals": "^3.0.0",
    "xml2js": "^0.4.23",
    "xregexp": "^5.0.2"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ],
    "rules": {
      "no-unused-vars": 0,
      "import/no-anonymous-default-export": 0,
      "jsx-a11y/anchor-has-content": 0,
      "no-unreachable": 0
    },
    "globals": {
      "__basedir": true,
      "page": true,
      "browser": true,
      "context": true,
      "jestPuppeteer": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-pipeline-operator": "^7.15.0",
    "@babel/plugin-transform-runtime": "^7.15.0",
    "@babel/preset-react": "^7.14.5",
    "babel-loader": "^8.2.2",
    "babel-plugin-jsdoc-runtime-typecheck": "^1.2.1",
    "babel-plugin-runtyper": "^0.4.0",
    "css-loader": "^6.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^5.3.2",
    "jest": "^27.0.6",
    "jest-puppeteer": "^5.0.4",
    "prettier": "2.3.1",
    "sass-loader": "^12.1.0",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.52.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.0",
    "webpack-license-plugin": "^4.1.2"
  },
  "prettier": {
    "quoteProps": "consistent"
  },
  "jest": {
    "preset": "jest-puppeteer",
    "testPathIgnorePatterns": [
      "/inflection/",
      "/node_modules/"
    ]
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      "@babel/react"
    ],
    "plugins": [
      [
        "@babel/plugin-transform-runtime"
      ],
      [
        "@babel/plugin-proposal-pipeline-operator",
        {
          "proposal": "fsharp"
        }
      ]
    ],
    "env": {
      "development": {
        "plugins": [
          [
            "@babel/transform-runtime"
          ],
          [
            "@babel/plugin-proposal-pipeline-operator",
            {
              "proposal": "fsharp"
            }
          ],
          [
            "jsdoc-runtime-typecheck",
            {
              "useStrict": true,
              "useDirective": false
            }
          ]
        ]
      }
    }
  }
}
