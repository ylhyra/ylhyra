{
  "name": "ylhyra",
  "version": "0.3.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git://github.com/ylhyra/ylhyra.git"
  },
  "license": "MIT",
  "private": false,
  "homepage": "https://ylhyra.is",
  "scripts": {
    "start_y": "export PROJECT=ylhyra && npm run start",
    "start": "export BROWSER=none && export NODE_ENV=development && export $(cat .env) && npx webpack serve --config scripts/webpack/app.webpack.js",
    "links": "./../ylhyra_content/scripts/cleanup.sh && export NODE_ENV=production && npm run build_server && npm run links_only",
    "links_only": "node build/server/ylhyra_server.js --generate-links",
    "sort_chapter_files": "npm run links && node build/server/ylhyra_server.js --sort_course_chapters && npm run links",
    "sitemap": "npm run links && npm run sitemap_only",
    "sitemap_only": "node build/server/ylhyra_server.js --sitemap",
    "vocabulary": "npm run build_server && npm run vocabulary_only",
    "vocabulary_es": "export DECK='_es' && npm run vocabulary",
    "vocabulary_da": "export DECK='_da' && npm run vocabulary",
    "vocabulary_no": "export DECK='_no' && npm run vocabulary",
    "vocabulary_only": "node build/server/ylhyra_server.js --import-vocabulary",
    "vocabulary_testing": "node build/server/ylhyra_server.js --import-vocabulary --testing",
    "generate_sentences": "npm run build_server && node build/server/ylhyra_server.js --generate-sentences",
    "prerender": "npm run build_server && npm run prerender_only",
    "prerender_only": "export NODE_ENV=production && node build/server/ylhyra_server.js --prerender",
    "prerender_single": "npm run build_server && npm run prerender_only",
    "build_app": "./scripts/app.build.sh && npm run inflection_styles",
    "build_server": "export NODE_ENV=production && node scripts/esbuild/esbuild.js",
    "server": "export NODE_ENV=development && nodemon --exec 'node scripts/esbuild/esbuild.js && node build/server/ylhyra_server.js' --watch src/ylhyra/server --watch **server.ts --watch package.json --watch src/inflection --watch src/documents/compile --watch build/links.json -e js,ts,json",
    "build_all": "./scripts/build_all.sh",
    "build_all_and_prerender": "npm run build_all && npm run prerender_only",
    "push_to_server": "./scripts/push_to_server.sh",
    "deploy": "./scripts/deploy.sh",
    "audio1": "./scripts/audio_processing/Pronunciation_step_1.sh",
    "audio3": "./scripts/audio_processing/Pronunciation_step_3.sh",
    "test": "npm run unit_tests && npm run integration_tests",
    "unit_tests": "jest --silent=false",
    "unit_tests_watch": "jest --watch --silent=false --detectOpenHandles",
    "integration_tests": "npm run build_app && npm run build_server && node -r esm src/ylhyra/tests/integrationTests/puppeteer.js",
    "integration_tests_only": "node -r esm src/ylhyra/tests/integrationTests/puppeteer.js",
    "start_test_server": "export SERVER_PORT=9999 && node build/server/ylhyra_server.js",
    "inflection_styles": "stylus --include src/inflection/styles/ < src/inflection/styles/index.styl > build//inflection_styles.css --compress",
    "compile_for_debugging": "node scripts/esbuild/esbuild_for_debugging.js",
    "flamegraph": "npm run compile_for_debugging && export DISABLE_LOGGING=1 && node ./../0x/cmd.js --tree-debug  --output-dir=/tmp/flamegraph -- node build/ts/debug.js",
    "fl:start": "run-p fl:dev:**",
    "fl:server": "node scripts/esbuild/esbuild.js --entry=src/flashcards/server.ts --out=build/server/flaschards.js",
    "fl:dev:frontend": "export PROJECT=flashcards && npm run start",
    "fl:dev:css": "npx tailwindcss -i src/flashcards/app/styles/tailwind/index.pcss -o ./src/flashcards/app/styles/tailwind/output.css --watch --postcss",
    "fl:dev:server": "export NODE_ENV=development && nodemon --exec 'npm run fl:server && node build/server/flaschards.js' --watch src/flashcards/**/*.server.ts",
    "fl:db": "npx prisma format && npx prisma db push && npx prisma generate"
  },
  "proxy": "http://localhost:9123",
