    "babel-plugin-jsdoc-runtime-typecheck": "^1.2.1",
    "babel-plugin-runtyper": "^0.4.0",
    "css-loader": "^6.2.0",
    "esbuild-loader": "^2.16.0",
    "esbuild-node-externals": "^1.3.0",
    "esbuild-plugin-ignore": "^1.1.0",
    "esbuild-stylus-loader": "^0.3.2",
    "eslint-config-react-app": "^6.0.0",
    "file-loader": "^6.2.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^5.3.2",
    "react-refresh": "^0.10.0",
    "style-loader": "^3.2.1",
    "stylus": "^0.55.0",
    "stylus-loader": "^6.1.0",
    "terser-webpack-plugin": "^5.2.4",
    "webpack": "^5.52.1",
    "webpack-bundle-analyzer": "^4.4.2",
    "webpack-cli": "^4.8.0",
    "webpack-dev-server": "^4.2.0",
    "webpack-license-plugin": "^4.1.2",
    "webpack-module-hot-accept": "^1.0.5"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      ">0.2%"
    ]
  },
  "prettier": {
    "quoteProps": "consistent"
  },
  "jest": {
    "moduleDirectories": [
      "node_modules",
      "src"
    ],
    "testEnvironment": "jsdom",
    "testPathIgnorePatterns": [
      "/inflection/",
      "/node_modules/"
    ],
    "testURL": "http://localhost:9123",
    "testRegex": "src/tests/unitTests/.*\\.test\\.[jt]sx?$",
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/src/tests/unitTests/mockIgnore.js"
    }
  }
}
