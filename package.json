{
  "name": "ylhyra",
  "version": "0.3.0",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git://github.com/ylhyra/ylhyra.git"
  },
  "license": "MIT",
  "private": false,
  "homepage": "https://ylhyra.is",
  "xxtype": "module",
  "scripts": {
    "start": "export BROWSER=none && react-scripts start",
    "links": "node build/server/ylhyra_server.js --compile-content",
    "vocabulary": "node build/server/ylhyra_server.js --import-vocabulary",
    "vocabulary_testing": "node build/server/ylhyra_server.js --import-vocabulary --testing",
    "prerender": "export NODE_ENV=production && rm -f build/prerender/* && node build/server/ylhyra_server.js --prerender",
    "build_and_prerender": "./scripts/build_and_prerender.sh",
    "build_app": "./scripts/app.build.sh",
    "build_server": "export NODE_ENV=production && webpack --config ./scripts/server.webpack.js",
    "server": "export NODE_ENV=development && webpack --config ./scripts/server.webpack.js --color --watch",
    "push_to_server": "./scripts/push_to_server.sh",
    "xxdeploy": "npm run build_and_prerender && npm run push_to_server"
  },
  "proxy": "http://localhost:9123",
  "dependencies": {
    "@google-cloud/translate": "^6.2.2",
    "@hcaptcha/react-hcaptcha": "^0.3.6",
    "argon2": "^0.28.2",
