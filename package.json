    "atob": "^2.1.2",
    "axios": "^0.21.1",
    "body-parser": "^1.19.0",
    "btoa": "^1.2.1",
    "chalk": "^4.1.1",
    "cologne-phonetic": "^1.0.1",
    "cookie-session": "^1.4.0",
    "core-js": "^3.14.0",
    "cors": "^2.8.5",
    "critical": "^4.0.0",
    "diacritics": "^1.3.0",
    "diff": "^5.0.0",
    "dotenv": "^10.0.0",
    "emoji-strip": "^1.0.1",
    "express": "^4.17.1",
    "express-useragent": "^1.0.15",
    "express-ws": "^4.0.0",
    "file-extension": "^4.0.5",
    "form-data": "^3.0.0",
    "formik": "^2.2.9",
    "html-entities": "^1.4.0",
    "is-boolean-attribute": "0.0.1",
    "is-empty-object": "^1.1.1",
    "jquery": "^3.6.0",
    "js-sha256": "^0.9.0",
    "js-yaml": "^4.1.0",
    "json-stable-stringify": "^1.0.1",
    "line-by-line": "^0.1.6",
    "marked": "^2.0.7",
