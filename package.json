    "multer": "^1.4.2",
    "mysql": "^2.18.1",
    "node-sass": "^6.0.0",
    "nodemailer": "^6.6.1",
    "nodemon-webpack-plugin": "^4.5.2",
    "omit-empty": "^1.0.0",
    "performance-now": "^2.1.0",
    "react": "^17.0.2",
    "react-attr-converter": "^0.3.1",
    "react-dom": "^17.0.2",
    "react-mic": "^12.4.6",
