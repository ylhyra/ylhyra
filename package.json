    "esm": "^3.2.25",
    "express": "^5.0.0-beta.1",
    "express-rate-limit": "^5.3.0",
    "express-slow-down": "^1.4.0",
    "express-useragent": "^1.0.15",
    "express-ws": "^4.0.0",
    "file-extension": "^4.0.5",
    "fork-ts-checker-webpack-plugin": "^7.2.1",
    "formik": "^2.2.9",
    "get-parameter": "^1.0.8",
    "html-entities": "^1.4.0",
