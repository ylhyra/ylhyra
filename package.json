  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 2 Chrome versions"
    ]
  },
  "prettier": {
    "quoteProps": "consistent",
    "trailingComma": "all",
    "plugins": [
      "prettier-plugin-jsdoc",
      "prettier-plugin-prisma"
    ]
  },
  "prisma": {
    "schema": "src/flashcards/database/schema.prisma"
  },
  "jest": {
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "testPathIgnorePatterns": [
      "/ylhyra/tests/",
      "/node_modules/"
    ],
    "testEnvironment": "jsdom",
    "testEnvironmentOptions": {
      "url": "http://localhost:9123"
    },
    "testRegex": "src/.*\\.test\\.tsx?$",
    "moduleNameMapper": {
      "\\.(css)$": "<rootDir>/src/modules/tests/mockIgnoreFileType.ts"
    }
  }
